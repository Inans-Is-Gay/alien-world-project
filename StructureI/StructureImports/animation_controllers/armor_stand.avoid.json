{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.armor_stand.avoid": {
			"states": {
                "default":{
                    "transitions": [
						{
							"azalea": "query.has_any_family('avoid_jungle_wood')&&query.life_time>2.0&&!query.is_avoiding_block"
						}
					]
                },
				"azalea":{
					"on_entry":[
						"@s avoid_azalea_leaves"
					]
				}
			}
		}
	}
}
